{% extends 'base.html.twig' %}
{% block stylesheets %}
<link rel="stylesheet" href="{{asset(" css/mon.css")}}" />
{% endblock %}

{% block title %}Airbnb{% endblock %}

{% block body %}

<!-- Navbar -->
<nav class="navbar navbar-expand-lg navbar-light bg-body-tertiary">
    <!-- Container wrapper -->
    <div class="container">
        <!-- Navbar brand -->
        <a class="navbar-brand me-2" href="https://www.airbnb.fr//">
            <img src="data:image/png;base64,"
                height="16" alt="MDB Logo" loading="lazy" style="margin-top: -1px;" />
        </a>

        <!-- Toggle button -->
        <button data-mdb-collapse-init class="navbar-toggler" type="button" data-mdb-target="#navbarButtonsExample"
            aria-controls="navbarButtonsExample" aria-expanded="false" aria-label="Toggle navigation">
            <i class="fas fa-bars"></i>
        </button>

        <!-- Collapsible wrapper -->
        <div class="collapse navbar-collapse" id="navbarButtonsExample">
            <!-- Left links -->
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link" href="#">Dashboard</a>
                </li>
            </ul>
            <!-- Left links -->

            <div class="d-flex align-items-center">
                <button data-mdb-ripple-init type="button" class="btn btn-link px-3 me-2">
                    Login
                </button>
                <button data-mdb-ripple-init type="button" class="btn btn-primary me-3">
                    Sign up for free
                </button>
                <a data-mdb-ripple-init class="btn btn-dark px-3" href="https://github.com/mdbootstrap/mdb-ui-kit"
                    role="button"><i class="fab fa-github"></i></a>
            </div>
        </div>
        <!-- Collapsible wrapper -->
    </div>
    <!-- Container wrapper -->
</nav>
<!-- Navbar -->


<div class="card border border-light border-warning shadow-0 mb-3 box-custom">
    <img src="https://mdbcdn.b-cdn.net/img/new/slides/017.webp" class="card-img" alt="Stony Beach" />
    <div class="card-img-overlay">
        <h4 class="card-title card-text box-custom">En savoir plus sur les Coups de cœur voyageurs.</h4>
        <div class="card-body">
            <a href="#" class="card-link">Logement</a><a href="#" class="card-link">Experiences</a><a href="#"
                class="card-link">Experiences en ligne</a>
            <p class="text-white-50 mb-5 card-text">Please enter your login and password!</p>
            <button class="btn btn-outline-light btn-lg px-5" type="submit">Login</button>
        </div>
    </div>

    <div class="row row-cols-1 row-cols-md-2 g-4 mt-4">
        <div class="col">
            <div class="card">
                <img src="https://a0.muscache.com/im/pictures/58c8280a-fc5d-4dbd-ab83-2037404b323b.jpg?im_w=720"
                    class="card-img-top" alt="Hollywood Sign on The Hill" />
                <div class="card-body">
                    <h5 class="card-title-bg">Appartement • St-Raphael,France</h5>
                    <div data-testid="listing-card-text">
                        <span class="dir dir-dir">Particulier</span>
                        <span class="dir dir-dir">5-10 févr.</span>
                    </div>
                    <h5 class="card-text-bg">283 €</h5>
                    <p>par nuit • 1 415€ au total</p>
                </div>
            </div>
        </div>
        <div class="col">
            <div class="card">
                <img src="https://a0.muscache.com/im/pictures/miso/Hosting-619539197654265580/original/d31e7907-9083-4b93-b589-f90568e1da28.jpeg?im_w=720"
                    class="card-img-top" alt="Palm Springs Road" />
                <div class="card-body">
                    <h5 class="card-title-bg">Appartement • Calafell,Espagne</h5>
                    <div data-testid="listing-card-text">
                        <span class="dir dir-dir">Particulier</span>
                        <span class="dir dir-dir">8-22 avrl.</span>
                    </div>
                    <h5 class="card-text-bg">187 €</h5>
                    <p>par nuit • 2 805€ au total</p>

                </div>
            </div>
        </div>
        <div class="col">
            <div class="card">
                <img src="https://a0.muscache.com/im/pictures/miso/Hosting-710699648970148782/original/8948cf2b-d98c-47f3-98b9-c7dc5dd7dc1f.jpeg?im_w=720"
                    class="card-img-top" alt="Los Angeles Skyscrapers" />
                <div class="card-body">
                    <h5 class="card-title-bg">Villa • Budens,Portugal</h5>
                    <div data-testid="listing-card-text">
                        <span class="dir dir-dir">Professionel</span>
                        <span class="dir dir-dir">20-25 juin</span>
                    </div>
                    <h5 class="card-text-bg">1284 €</h5>
                    <p>par nuit • 6 420€ au total</p>

                </div>
            </div>
        </div>
        <div class="col">
            <div class="card">
                <img src="https://a0.muscache.com/im/pictures/33caa4bf-8d06-423f-87ea-22097ee4857c.jpg?im_w=720"
                    class="card-img-top" alt="Skyscrapers" />
                <div class="card-body">
                    <h5 class="card-title-bg">Appartement • Moneglia,Italie</h5>
                    <div data-testid="listing-card-text">
                        <span class="dir dir-dir">Professionel</span>
                        <span class="dir dir-dir">14-30 dec.</span>
                    </div>
                    <h5 class="card-text-bg">229 €</h5>
                    <p>par nuit • 3 893€ au total</p>

                </div>
            </div>
        </div>
    </div>

    <footer id="footer">
        <nav class="nav-footer">
            <ul class="nav-ul-footer">
                <p><a href="#">Politique des cookies</a></p>
                <p><a href="#">CGV</a></p>
                <p><a href="#">Mentions légales</a></p>
                <p><a href="#">Politique</a></p>
            </ul>
    </footer>
    {% endblock %}